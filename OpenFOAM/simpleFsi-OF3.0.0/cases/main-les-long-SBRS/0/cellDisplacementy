FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      cellDisplacementy;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    cylinder
    {
        type            fixedValue;
        value           uniform 0;
    }
    
    inlet
    {
        type            zeroGradient;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    channel
    {
        type            uniformFixedValue;
        uniformValue    constant 0;
    }
    
    defaultFaces
    {
        type            empty;
    }
}

